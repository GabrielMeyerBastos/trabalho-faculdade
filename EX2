from re import I


print('='*70)

numeros = list()

print(f'digite 5 numeros: ')


for  numero in range(5):
    
    numeros.append(float(input(f'digite o {numero+1}ª  numero: ')))
    
print('a soma dos numeros é :{}'.format(sum(numeros)))

def mp(numeros):
    
    resultado = numeros[0]
    
    for i in range(1, len(numeros)):
        
        resultado = resultado * numeros[i]

    return resultado

print('a multiplicação é : {}'.format(mp(numeros)))

print('='*70)
